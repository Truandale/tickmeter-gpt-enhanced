<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.8"/>
  </startup>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Data Source = |SQL/CE|"/>
  </connectionStrings>
  <runtime>
    <assemblyBinding xmlns="urn:schemas-microsoft-com:asm.v1">
    </assemblyBinding>
  </runtime>
  <mdaConfig>
    <assistants>
      <disconnectedContext disable="true"/>
      <contextSwitchDeadlock disable="true"/>
      <gcUnmanagedToManaged disable="true"/>
      <invalidCERCall disable="true"/>
      <raceOnRCWCleanup disable="true"/>
    </assistants>
  </mdaConfig>
</configuration>
